<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.xx.style">

    <uses-permission android:name="android.permission.INTERNET"/>
    <uses-permission android:name="android.permission.WRITE_EXTERNAL_STORAGE"/>
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE"/>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:name=".base.BaseApplication"
        android:theme="@style/AppTheme">
        <activity android:name=".view.user.SplashActivity"
                android:icon="@mipmap/ic_launcher"
            android:theme="@style/splash_style">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
        <activity android:name=".view.learning.LearningActivity" />
        <activity android:name=".view.learning.LearningWebActivity" />
        <activity android:name=".view.learning.LearningListActivity" />
        <activity android:name=".view.learning.LearningDescActivity" />
        <activity android:name=".view.user.LockingActivity" />
        <activity android:name=".view.loving.LovingActivity" />
        <activity android:name=".view.loving.LovingCameraActivity" />
        <activity android:name=".view.loving.PhotoListActivity" />
        <activity android:name=".view.weiget.WeigetActivity" />

        <meta-data
            android:name="android.max_aspect"
            android:value="2.2" />
    </application>

</manifest>